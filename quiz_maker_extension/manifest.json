{
"manifest_version": 2,
"name": "QuizzerGPTube",
"description": "Create quiz from Youtube videos",
"version": "1.0",
"browser_action": {
"default_icon": "icon-1.png (1).png",
"default_popup": "popup.html"
},
"content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "css": ["styles.css"]
    }
  ]
}